<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB//EN" "http://www.bea.com/servers/wls700/dtd/weblogic-ejb-jar.dtd">
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>Budapest</ejb-name>
        <entity-descriptor>
            <pool></pool>
            <entity-cache></entity-cache>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
            <entity-clustering></entity-clustering>
        </entity-descriptor>
        <transaction-descriptor></transaction-descriptor>
        <local-jndi-name>com.appiancorp.ejb.BudapestHome</local-jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>Telephone</ejb-name>
        <entity-descriptor>
            <pool></pool>
            <entity-cache></entity-cache>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
            <entity-clustering></entity-clustering>
        </entity-descriptor>
        <transaction-descriptor></transaction-descriptor>
        <local-jndi-name>com.appiancorp.ejb.TelephoneHome</local-jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>

